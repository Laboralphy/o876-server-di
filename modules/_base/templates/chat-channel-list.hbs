{{!--
Displays a list of joined channel
@param channelList {{ name: string, active: boolean }[]}
--}}
{{#table}}
    {{t "chat.channelListItemName"}}, {{t "chat.channelListItemActive"}}
    {{#each channelList}}
        {{#line}}
            {{#if active}}{{color "#2f2"}}{{ name }}, {{t "chat.channelActive"}}
            {{ else }}{{color "#999"}}{{ name }}, {{t "chat.channelInactive"}}{{/if}}{{color ""}}
        {{/line}}
    {{/each}}
{{/table}}
